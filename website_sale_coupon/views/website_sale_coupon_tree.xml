<?xml version="1.0" encoding="utf-8"?>

<odoo>
    <record id="website_sale_coupon_tree" model="ir.ui.view" >
        <field name="name">Website coupon tree</field>
        <field name="model">website.sale.coupon</field>
        <field name="mode">primary</field>
        <field name="arch" type="xml">
            <tree string="Website coupons">
                <field name="name"/>
                <field name="code"/>
                <field name="available"/>
                <field name="value"/>
                <field name="type"/>
            </tree>
        </field>
    </record>

</odoo>