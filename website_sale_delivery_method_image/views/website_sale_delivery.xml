<?xml version="1.0" encoding="UTF-8"?>

<odoo>
    <template id="payment_delivery_image" inherit_id="website_sale_delivery.payment_delivery_methods">
        <xpath expr="//label" position="before">
            <t t-if="delivery.delivery_image">
                <img t-attf-src="data:image/*;base64,{{delivery.delivery_image}}" t-attf-style="max-width: 60px; display: inline-block"/>
            </t>
            <!--
            <span t-field="delivery.delivery_image" t-att-title="delivery.name" t-field-options='{"widget": "image", "style":"max-width: 60px; display: inline-block"}'/>-->
        </xpath>_
    </template>
</odoo>
